<html><head>
<title>DnDTool</title>
<script src="/socket.io/socket.io.js"></script>
<script>var socket = io('/DnDNotesMain');</script>
<script>


socket.on('RcvMainSessionID', function(MainSessionID){
	sessionStorage.setItem("MainSessionID", MainSessionID);
	console.log("Main Session ID: " + sessionStorage.getItem("MainSessionID"));
});

var SessionID;

function pageLoad(){	
	var AccountInfo = <%- AccountInfo %>;
	console.log("woof");
	console.log(AccountInfo);
	console.log("meow");
	//set the RID into session storage

	SessionID = sessionStorage.getItem("MainSessionID");
	if (SessionID == null){
		console.log("First Page Load");
		socket.emit("GetMainSessionID");
		
	}
	else{
		console.log("Second Page Load - Session ID: " + SessionID);
	}
}

</script>
</head>

<body bgcolor="#BEC6DD" onLoad="pageLoad()">

<iframe id="lobby" src="/Lobby" style="position:fixed; top:0; left:0; bottom:0; right:0; width:100%; height:100%; border:none; margin:0; padding:0; overflow:hidden; z-index:999999;">
    Your browser doesn't support iframes
</iframe>

</body></html>
